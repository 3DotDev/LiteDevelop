    </ItemGroup>
    <ItemGroup Id = "Statusbar">
      <Item Id = "CurrentLine">Linje $(line)</Item>
      <Item Id = "CurrentColumn">Kolonne $(column)</Item>
    </ItemGroup>
  </ItemGroup>

  <ItemGroup Id = "GeneralSettingsControl">
    <Item Id = "ShowLineNumbers">Vis linjenummer</Item>
    <Item Id = "WordWrap">Auto formater tekst</Item>
