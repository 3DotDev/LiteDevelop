<?xml version="1.0" encoding="utf-8" ?>
<UILanguagePack>

  <!-- 
  ==================================
  Code editor 
  ==================================
  -->
  
  <ItemGroup Id = "CodeEditorControl">
    <ItemGroup Id = "ContextMenu">
      <Item Id = "Cut">Klip</Item>
      <Item Id = "Copy">Kopier</Item>
      <Item Id = "Paste">Sæt ind</Item>
      <Item Id = "SelectAll">Vælg alt</Item>
    </ItemGroup>
  </ItemGroup>

  <ItemGroup Id = "CodeEditorExtension">
    <Item Id = "GeneralSettings">Generelt</Item>
    <Item Id = "AutoCompleteSettings">Auto korrektur</Item>
    <ItemGroup Id = "Toolbar">
      <Item Id = "ShowSuggestions">Vis foreslag</Item>
      <Item Id = "CommentRegion">Kommenter region</Item>
