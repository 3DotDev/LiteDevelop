        <Item Id = "PressingCtrlSpace">Ctrl+Mellemrum er trykket</Item>
      </ItemGroup>
      <Item Id = "CommitItemOnKeyChars">Indsæt valgte element når en af disse taster er trykket:</Item>
      <Item Id = "CommitItemOnSpaceBar">Indsæt valgte element når mellemrum er trykket</Item>
    </ItemGroup>
    <Item Id = "AutoCompleteCodeBlocks">Færdiggør automatisk kodeblokke (e.g. ( ) [ ] " " ' ')</Item>
    <Item Id = "AutoAddParantheses">Tilføj automatisk paranteser når en metode er indsat</Item>
  </ItemGroup>
  
</UILanguagePack>
